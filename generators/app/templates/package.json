{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "repository": {
    "type": "git",
    "url": "git+<%= git %>.git"
  },
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "<%= git %>/issues"
  },
  "homepage": "<%= git %>#README",
  "private": true,
  "scripts": {
    "test": "gulp test",
    "test-karma": "node_modules/karma-cli/bin/karma start karma.conf.js",
    "test-karma-single-run": "node_modules/karma-cli/bin/karma start karma.conf.js --single-run",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "node_modules/protractor/bin/protractor e2e-tests/protractor.conf.js",
    "deploy": "cd ~/workspace/ansible/ansible-play/books/ && ansible-playbook django_deploy.yml -e \"host=<%= deployHost %> project=<%= name %>\" --ask-vault-pass",
    "server_upgrade": "cd ~/workspace/ansible/ansible-play/books/ && ansible remotes -m apt -a \"upgrade=yes\" -b -K"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-resource": "^1.5.7",
    "angular-route": "^1.5.7",
    "angular-sanitize": "^1.5.7",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.3",
    "gentelella": "^1.3.0",
    "jquery": "^2.2.4",
    "ng-gentelella": "^0.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "concurrent-transform": "^1.0.0",
    "del": "^2.2.0",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-google-webfonts": "^0.0.14",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.11.0",
    "gulp-less": "^3.0.5",
    "gulp-minify-css": "^1.2.4",
    "gulp-notify": "^2.2.0",
    "gulp-nsp": "^2.4.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "^4.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^6.3.0"
  }
}
